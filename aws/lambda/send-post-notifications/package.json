{
  "name": "send-post-notifications",
  "version": "0.0.1",
  "description": "send regular post notification email",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jschmidtnj/ewaab.git"
  },
  "author": {
    "name": "joshua"
  },
  "keywords": [
    "backend",
    "post-notifications"
  ],
  "files": [
    "lib"
  ],
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "start": "node --trace-deprecation lib/index",
    "dev": "tsc-watch --onSuccess \"yarn postbuildWatch\"",
    "clean": "rimraf lib && rimraf coverage",
    "format": "prettier --config prettier.config.js --ignore-path .gitignore --write \"{src,__tests__}/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "precommit": "yarn lint && yarn format",
    "predeploy": "yarn build && ./build_zip.sh",
    "prepublishOnly": "yarn build",
    "printTypescriptVersion": "echo Using TypeScript && tsc --version",
    "prebuild": "yarn precommit && yarn printTypescriptVersion",
    "build": "ttsc",
    "test": "jest",
    "coverage": "jest --coverage",
    "postbuildWatch": "yarn printTypescriptVersion && yarn start",
    "watch:test": "jest --watch"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.11.0",
    "aws-sdk": "^2.819.0",
    "class-validator": "^0.12.2",
    "cosmiconfig": "^7.0.0",
    "dompurify": "^2.2.6",
    "dotenv": "^8.2.0",
    "elastic-builder": "^2.11.0",
    "exit-hook": "^2.2.0",
    "handlebars": "^4.7.7",
    "http-status-codes": "^2.1.4",
    "jsdom": "^16.5.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "nanoid": "^3.1.20",
    "pg": "^8.5.1",
    "showdown": "^1.9.1",
    "ts-transformer-keys": "^0.4.3",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.66",
    "@types/dompurify": "^2.2.1",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.19",
    "@types/jsdom": "^16.2.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.14",
    "@types/showdown": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "apollo-server-express": "^2.21.1",
    "aws-lambda": "^1.0.6",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.9",
    "eslint-plugin-prefer-arrow": "^1.2.2",
    "eslint-plugin-prettier": "^3.3.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.2.9",
    "tslint": "^6.1.3",
    "ttypescript": "^1.5.12",
    "typescript": "^4.1.3",
    "typescript-rest": "^3.0.2"
  },
  "resolutions": {
    "graphql": "^15.4.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "jest": {
    "preset": "ts-jest"
  }
}
