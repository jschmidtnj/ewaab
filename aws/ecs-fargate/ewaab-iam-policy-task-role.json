{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "ECAllowSpecific",
      "Effect": "Allow",
      "Action": [
        "elasticache:*"
      ],
      "Resource": "*"
    },
    {
      "Effect": "Allow",
      "Action": "s3:*",
      "Resource": [
        "arn:aws:s3:::ewaab-emails",
        "arn:aws:s3:::ewaab-emails/*",
        "arn:aws:s3:::ewaab-files",
        "arn:aws:s3:::ewaab-files/*"
      ]
    },
    {
      "Effect": "Allow",
      "Action": [
        "ses:*"
      ],
      "Resource": "*"
    }
  ]
}