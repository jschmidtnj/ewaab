  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "start": "node --trace-deprecation lib/index",
    "dev": "yarn prebuild && tsc-watch --compiler ttypescript/bin/tsc --onSuccess \"yarn postbuildWatch\"",
    "clean": "rimraf lib && rimraf coverage && rimraf dist_swagger",
    "format": "prettier --config prettier.config.js --ignore-path .gitignore --write \"{src,__tests__}/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "precommit": "yarn lint && yarn format && yarn swagger",
    "predeploy": "yarn build",
    "prepublishOnly": "yarn build",
    "printTypescriptVersion": "echo Using TypeScript && ttsc --version",
    "prebuild": "yarn precommit && yarn printTypescriptVersion",
    "build": "ttsc",
    "test": "jest",
    "coverage": "jest --coverage",
    "postbuildWatch": "yarn printTypescriptVersion && yarn start",
    "watch:test": "jest --watch",
    "swagger": "swaggerGen -c ./swaggerConf.yml && cp dist_swagger/swagger.yaml swagger.yml"
  },
